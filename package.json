    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "25.0.7",
        "@rollup/plugin-json": "6.1.0",
        "@rollup/plugin-node-resolve": "15.2.3",
        "@types/cheerio": "0.22.35",
        "@types/jest": "29.5.12",
        "@types/luxon": "3.4.2",
        "@types/node": "20.12.7",
        "@typescript-eslint/eslint-plugin": "7.8.0",
        "@typescript-eslint/parser": "7.8.0",
        "await-to-js": "3.0.0",
        "cheerio": "1.0.0-rc.12",
        "eslint": "9.1.1",
        "eslint-plugin-prettier": "5.1.3",
        "file-type": "16.5.3",
        "html-entities": "2.5.2",
        "jest": "29.7.0",
        "luxon": "3.4.4",
        "node-fetch": "3.3.2",
        "prettier": "3.2.5",
        "rollup": "4.17.2",
        "rollup-plugin-dts": "6.1.0",
        "rollup-plugin-terser": "7.0.2",
        "rollup-plugin-typescript2": "0.36.0",
        "ts-jest": "29.1.2",
        "ts-node": "10.9.2",
        "typedoc": "0.25.13",
        "typescript": "5.4.5"
    },
    "scripts": {
        "build": "rollup --config",
        "docs": "typedoc --options typedoc.json --disableSources",
